    "babel-preset-react-native": "1.9.1",
    "jest": "19.0.2",
    "jest-environment-node-debug": "^2.0.0",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
